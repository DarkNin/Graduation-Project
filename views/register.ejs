<%- include('header') %>
<link rel="stylesheet" href="/css/form-styles.css">
<div class="form-panel container">
    <div class="row">
        <form class="col s12" method="post">
            <div class="input-field">
                <input type="text" id="nickname" placeholder="请输入昵称" name="nickname">
                <label for="nickname">昵称</label>
            </div>
            <div class="input-field">
                <input type="password" id="password" placeholder="请输入密码" name="password">
                <label for="password">密码</label>
            </div>
            <div class="input-field">
                <input type="password" id="_password" placeholder="请再次输入密码" name="_password">
                <label for="_password">确认密码</label>
            </div>
            <button class="btn" type="submit">注册账号</button>
        </form>
    </div>
</div>


<%- include('footer') %>